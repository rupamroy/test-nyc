{
    "check-coverage": true,
    "lines": 90,
    "statements": 90,
    "functions": 90,
    "branches": 90,
    "all": true,
    "include": [
        "server"
    ],
    "exclude": [
        "**/index.ts",
        "**/tests/*.ts",
        "**/*.model.ts",
        "**/*.module.ts",
        "**/*.spec.ts"
    ],
    "reporter": [
        "html",
        "text-summary"
    ],
    "extension": [
        ".ts"
    ]
}
